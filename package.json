{
    "name": "frontend",
    "version": "1.0.0",
    "main": "index.js",
    "scripts": {
        "build:linux.old": "tsc && cp -r ./src/* ./out/ ",
        "build": "npm run clean && tsc && ./copy.sh && npm run optimize",
        "start": "npm run build && node server/index.js",
        "build:windows": "tsc && copy.cmd && npm run optimize",
        "start:windows": "npm run build:windows && node server/index.js",
        "clean": "rm -rf ./out",
        "optimize": "npx tailwindcss -i ./src/css/tailwind.min.css -o ./out/css/tailwind.min.css"
    },
